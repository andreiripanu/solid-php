# https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties
# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines
[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# Indentation
indent_style = space
indent_size = 2
